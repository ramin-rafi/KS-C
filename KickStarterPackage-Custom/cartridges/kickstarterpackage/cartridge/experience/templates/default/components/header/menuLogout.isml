<isif condition="${pdict.CurrentCustomer.authenticated}">
    <li>
        <a href="${URLUtils.url('Login-Logout')}">
            <strong>${Resource.msg('link.header.logout', 'account', null)}</strong>
        </a>
    </li>
    <iselse/>
    <li>
        <a href="${URLUtils.https('Account-Show')}">${Resource.msg('link.header.login', 'account', null)}</a>
    </li>
</isif>
